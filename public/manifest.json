{
    "manifest_version": 2,
    "name": "Dev Assistant: Modify Request, User Agent,etc",
    "short_name": "Dev Assist",
    "version": "0.7",
    "description": "Helps to manage storage and modify / control http(s) requests, tabs, user agents etc by configuring various filters and actions.",
    "icons": {
        "128": "icon_128.png",
        "32": "icon_32.png"
    },
    "browser_action": {
        "default_icon": {
            "128": "icon_128.png",
            "32": "icon_32.png"
        },
        "default_title": "Dev Assistant",
        "default_popup": "menu.html"
    },
    "background": {
        "page": "background.html",
        "persistent": true
    },
    "commands": {
        "open-ja-dashboard": {
            "suggested_key": {
                "default": "Alt+D",
                "mac": "Command+Shift+D"
            },
            "description": "Open Dev Assistant dashboard page"
        }
    },
    "content_security_policy": "script-src 'self' ; object-src 'self'; img-src 'self';",
    "offline_enabled": true,
    "permissions": [
        "storage",
        "unlimitedStorage",
        "webRequest",
        "webRequestBlocking",
        "tabs",
        "proxy",
        "http://*/",
        "https://*/"
    ],
    "optional_permissions": [],
    "author": "Shridhar",
    "key": ""
}